{
  "name": "nocli-ssr",
  "version": "0.0.1",
  "description": "nocli-ssr",
  "scripts": {
    "server-web": "lite-server",
    "dev-web": "rimraf wwwroot/* && concurrently --kill-others \"webpack --config config/webpack.dev.js\" \"npm run server-web\"",
    "server-adaptive": "set IS_ADAPTIVE=true && npm run server-web",
    "dev-web:ssr": "concurrently --kill-others \"webpack --config config/webpack-ssr.dev.js\" \"npm run server-web\"",
    "dev-adaptive": "set IS_ADAPTIVE=true && rimraf wwwroot/* && concurrently --kill-others \"webpack --config config/webpack.dev.js\" \"npm run server-adaptive\"",
    "prod": "rimraf wwwroot/* && webpack --config config/webpack.prod.js --bail && npm run build-old-browsers",
    "prod-iframe": "rimraf wwwroot/* && set ROOT_PATH=spa/ && webpack --config config/webpack.prod.js --bail && npm run build-old-browsers",
    "tslint": "tslint --config tslint.unused-imports.json --project tsconfig.json --type-check --force --fix src/app.web/app.web.module.ts",
    "dev-old-browsers": "set WATCH=true && rimraf wwwroot/* && concurrently --kill-others \"npm run build-old-browsers\" \"npm run server-web\"",
    "build-old-browsers": "webpack --config config/webpack.old-browser.js",
    "docker:build": "docker build -t mp-ssr .",
    "compile-app-and-dockerize": "npm run ngb:all && npm run docker:build",
    "ngb:app": "webpack --config ./webpack.config.app.js --progress",
    "ngb:ssr": "webpack --config ./webpack.config.ssr.js --progress",
    "ngb:ssr:stat": "webpack --config ./webpack.config.ssr.js --progress --json > stat.json",
    "ngb:server": "webpack --config ./webpack.config.server.js --progress",
    "ngb:all": "npm run ngb:app && npm run ngb:ssr && npm run ngb:server",
    "go": "node dist/server",
    "go:dev": "set ENV=dev && rimraf dist/browser/* && concurrently --kill-others \"webpack --config webpack.config.app.js\" \"lite-server -c bs-config.ssr.js\"",
    "go:dev:adaptive": "set ENV=dev && set IS_ADAPTIVE=true && rimraf dist/browser/* && concurrently --kill-others \"webpack --config webpack.config.app.js\" \"lite-server -c bs-config.ssr.js\""
  },
  "license": "MIT",
