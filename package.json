    "@nguniversal/express-engine": "6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "6.0.0",
    "@ngx-material-keyboard/core": "0.1.0",
    "@types/express": "4.11.1",
    "core-js": "2.5.3",
    "crypto-browserify": "3.11.0",
    "es6-promise": "4.1.1",
    "express": "4.16.3",
    "moment": "2.20.1",
    "moment-range": "3.1.1",
    "ng2-nouislider": "1.7.11",
